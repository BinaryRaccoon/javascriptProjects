import React from 'react';
import ReactDOM from 'react-dom';

class App extends React.Component {
  getNumber(number) {
  	return(
    	number*53
    );
  }
  getPhrase() {
  	return(
    	"phrase"
    );
  }
  render(){
  	const word="word"
  	return(
      <div>
        {word}
        <br />
        {this.getPhrase()}
        <br />
        {this.getNumber(5)}
      </div>
    );
  }
}

ReactDOM.render(
	<App />,
	mountNode
);